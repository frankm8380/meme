# Ignore node_modules if using npm packages
node_modules/

# Ignore OS generated files
.DS_Store
Thumbs.db

# Ignore VS Code settings
.vscode/
.history/

# Ignore log files and temporary files
*.log
*.tmp
*.swp
*.swo

# Ignore WordPress core files (you only want your custom code and plugins/themes)
wp-admin/
wp-includes/
license.txt
readme.html

# Ignore uploads (if you don't want media files in Git)
wp-content/uploads/

# Ignore cache files
wp-content/cache/
