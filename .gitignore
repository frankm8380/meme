# Ignore backup files
*.bak
*.backup
*.old

# Ignore environment files
.env
.env.local

# Ignore PHPStorm and JetBrains project files
.idea/

# Ignore Git itself
.git/

# Ignore composer dependencies if you're using Composer
vendor/

# Ignore build output (if any)
dist/
build/

# Ignore plugin and theme debug files
wp-content/debug.log

# Ignore temporary Git merge files
*.orig

# Ignore image files
images/

archive/
snippetsArchive
wpcode-snippets-export-*.json
